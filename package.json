{
  "name": "preact-wysiwyg-editor",
  "version": "1.0.12",
  "description": "simple wysiwyg editor using preact",
  "author": "Danny Braunstein<github.com/dannyblv>",
  "license": "MIT",
  "keywords": [
    "preact",
    "wysiwyg",
    "editor",
    "text editor",
    "rich text",
    "content editable",
    "html editor"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "files": [
    "/dist/*"
  ],
  "scripts": {
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build": "yarn run build:cjs && yarn run build:esm"
  },
  "dependencies": {
    "preact": "^10.23.1"
  },
  "peerDependencies": {
    "preact": "*"
  },
  "devDependencies": {
    "preact": "^10.23.1",
    "typescript": "^5.5.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dannyblv/preact-wysiwyg-editor.git"
  }
}
